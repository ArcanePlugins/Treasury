            "contributors": [
                {
                    "name": "lokka30",
                    "description": "Lead Treasury developer."
                },
                {
                    "name": "MrIvanPlays",
                    "description": "Treasury developer."
                },
                {
                    "name": "Jikoo",
                    "description": "Treasury developer."
                }
